<Window x:Name="output" x:Class="Blackjack.Results"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Blackjack"
        mc:Ignorable="d"
        Title="Results" Height="1080" Width="1317">
    <Window.Background>
        <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
            <GradientStop Color="#FF001D80" Offset="0"/>
            <GradientStop Color="#FF46AAFF" Offset="0.736"/>
        </LinearGradientBrush>
    </Window.Background>
    <Grid HorizontalAlignment="Left" Height="1070" VerticalAlignment="Top" Width="1307" Margin="0,0,0,-20">
        <Label x:Name="lbloutput" Content="" HorizontalAlignment="Left" Height="775" Margin="68,89,0,0" VerticalAlignment="Top" Width="1174" FontFamily="Franklin Gothic Demi" FontSize="48"/>
        <Button Content="OK" HorizontalAlignment="Left" Height="106" Margin="512,903,0,0" VerticalAlignment="Top" Width="320" FontSize="60" FontFamily="Imprint MT Shadow" Click="Button_Click"/>
    </Grid>

</Window>
